<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.ssh.pojo.ConsumeRecordTO" table="consume_record" schema="pet_db">
        <id name="id" column="id">
            <generator class="identity"/>
        </id>
        <property name="petId" column="petId"/>
        <property name="consumeDate" column="consumeDate"/>
        <property name="consumeMoney" column="consumeMoney"/>
        <property name="remark" column="remark"/>
        <property name="balance" column="balance"/>
        <property name="consumeType" column="consumeType"/>
    </class>
</hibernate-mapping>